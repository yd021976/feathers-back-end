{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "uploads": "../uploads/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "winston_log_level": "debug",
  "authentication": {
    "secret": "",
    "authStrategies": [
      "jwt",
      "local"
    ],
    "parseStrategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "entity": "user",
    "service": "users",
    "entityId": "_id",
    "cookie": {
      "domain": "localhost",
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    },
    "jwtOptions": {
      "header": {
        "type": "access"
      },
      "audience": "localhost",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1h"
    },
    "local": {
      "entity": "user",
      "service": "users",
      "usernameField": "name",
      "passwordField": "password",
      "idField": "_id"
    },
    "anonymous": {
      "service": "users",
      "idField": "_id"
    }
  },
  "nedb": "../data",
  "file-system": {
    "root": "../public",
    "routes": [
      {
        "path": "/",
        "forbidden": false,
        "requireAuth": true,
        "param": "",
        "permissions": {
          "canRead": [
            "all"
          ],
          "canWrite": [
            "admins",
            "users"
          ],
          "canDelete": [
            "admins"
          ]
        },
        "excludes": [
          "\\.html$"
        ]
      },
      {
        "path": "/uploads/:user/*child/titi",
        "forbidden": true
      },
      {
        "path": "/uploads/:user/*child",
        "requireAuth": true,
        "roles": [
          "users"
        ],
        "param": "AUTH_USER"
      },
      {
        "path": "/uploads/:user",
        "requireAuth": true,
        "roles": [
          "users"
        ],
        "param": "AUTH_USER"
      },
      {
        "path": "/uploads",
        "requireAuth": true,
        "roles": [
          "users"
        ]
      }
    ]
  }
}